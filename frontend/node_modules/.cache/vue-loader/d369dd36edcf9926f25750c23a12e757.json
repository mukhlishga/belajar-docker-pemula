: task.done }\"\r\n        >\r\n          <template v-slot:default>\r\n            <v-list-item-action>\r\n              <v-checkbox :input-value=\"task.done\" color=\"primary\"></v-checkbox>\r\n            </v-list-item-action>\r\n\r\n            <v-list-item-content>\r\n              <v-list-item-title\r\n                :class=\"{ 'text-decoration-line-through': task.done }\"\r\n              >\r\n                {{ task.note }}\r\n              </v-list-item-title>\r\n            </v-list-item-content>\r\n            <v-list-item-action>\r\n              <v-btn @click.stop=\"deleteTask(task.id)\" icon>\r\n                <v-icon color=\"grey lighten-1\">mdi-trash-can</v-icon>\r\n              </v-btn>\r\n            </v-list-item-action>\r\n          </template>\r\n        </v-list-item>\r\n        <v-divider></v-divider>\r\n      </div>\r\n    </v-list>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"../libs/axios\";\r\n\r\nexport default {\r\n  name: \"Todo\",\r\n  data() {\r\n    return {\r\n      newTaskTitle: \"\",\r\n      tasks: [],\r\n    };\r\n  },\r\n  async mounted() {\r\n    await this.fetchTasks();\r\n  },\r\n  methods: {\r\n    async fetchTasks() {\r\n      try {\r\n        const response = await axios.get(\"/todo\");\r\n        this.tasks = response.data;\r\n      } catch (error) {\r\n        console.dir(error);\r\n      }\r\n    },\r\n    async addTask() {\r\n      let newTask = {\r\n        note: this.newTaskTitle,\r\n        done: false,\r\n      };\r\n      this.tasks.push(newTask);\r\n      this.newTaskTitle = \"\";\r\n\r\n      try {\r\n        await axios.post(\"/todo\", newTask);\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n    async doneTask(id) {\r\n      let task = this.tasks.filter((task) => task.id == id)[0];\r\n      task.done = !task.done;\r\n\r\n      try {\r\n        await axios.put(\"/todo\", task);\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n    async deleteTask(id) {\r\n      this.tasks = this.tasks.filter((task) => task.id !== id);\r\n\r\n      try {\r\n        await axios.delete(\"/todo\", { data: { id: id } });\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}